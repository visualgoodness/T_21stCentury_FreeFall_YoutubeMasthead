package{	import flash.events.MouseEvent;	import flash.display.MovieClip;	public class ResolveFrame extends InitialFrameOverlay	{		public function ResolveFrame()		{			channel_title_mc.mouseEnabled = false;			ending_title_mc.enabled = false;						bg_mc.buttonMode = true;			bg_mc.addEventListener(MouseEvent.CLICK, bgClicked);			channel_btn.addEventListener(MouseEvent.CLICK, channelBtnClicked);		}				private function bgClicked(e:MouseEvent):void		{			trace("---> " + this + " bgClicked");			dispatchEvent(new BannerEvent(BannerEvent.BACKGROUND_CLICKED));		}				private function channelBtnClicked(e:MouseEvent):void		{			trace("---> " + this + " channelBtnClicked");			dispatchEvent(new BannerEvent(BannerEvent.CHANNEL_BTN_CLICKED));		}	}}