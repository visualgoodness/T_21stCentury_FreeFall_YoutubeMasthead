package{	import flash.events.MouseEvent;	import flash.display.MovieClip;	public class InitialFrameOverlay extends MovieClip	{		private var _endingBtns:Array;				public function InitialFrameOverlay()		{			_endingBtns = [						    this["ending_btn_0"],						    this["ending_btn_1"],						    this["ending_btn_2"]						  ];			for (var i:uint = 0; i < _endingBtns.length; i++)			{				var btn:MovieClip = _endingBtns[i] as MovieClip;				if (btn.currentFrame != i+1) btn["btn_text_mc"].gotoAndStop(i+1);				btn.buttonMode = true;				btn.addEventListener(MouseEvent.CLICK, btnClicked);			}			this["get_quote_btn"].addEventListener(MouseEvent.CLICK, quoteBtnClicked);		}				private function quoteBtnClicked(e:MouseEvent):void		{			trace("---> " + this + " quoteBtnClicked");			dispatchEvent(new BannerEvent(BannerEvent.GET_QUOTE_CLICKED));		}				private function btnClicked(e:MouseEvent):void		{			var index:int = -1;			for (var i:uint = 0; i < _endingBtns.length; i++)			{				if (MovieClip(e.currentTarget).name == _endingBtns[i].name)				{					index = i;					break;				}			}			trace("---> " + this + " btnClicked = " + index);			dispatchEvent(new BannerEvent(BannerEvent.ENDING_BTN_CLICKED, index));		}	}}