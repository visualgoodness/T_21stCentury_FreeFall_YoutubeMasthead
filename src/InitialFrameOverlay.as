package{	import flash.events.MouseEvent;	import flash.display.MovieClip;	public class InitialFrameOverlay extends MovieClip	{		private var _endingBtns:Array;				public function InitialFrameOverlay()		{			_endingBtns = [ ending_btn_0, ending_btn_1, ending_btn_2 ];			for (var i:uint = 0; i < _endingBtns.length; i++)			{				var btn:MovieClip = _endingBtns[i] as MovieClip;				btn.buttonMode = true;				btn.addEventListener(MouseEvent.CLICK, btnClicked);			}		}				private function btnClicked(e:MouseEvent):void		{			var index:int = -1;			for (var i:uint = 0; i < _endingBtns.length; i++)			{				if (MovieClip(e.currentTarget).name == _endingBtns[i].name)				{					index = i;					break;				}			}			dispatchEvent(new CollapsedEvent(CollapsedEvent.ENDING_BTN_CLICKED, index));		}	}}